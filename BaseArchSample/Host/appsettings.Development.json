{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Elasticsearch" ],
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Elasticsearch",
              "Args": {
                "nodeUris": "http://elastic:aA123456@localhost:9200",
                "indexFormat": "applicationName-serviceName-{0:yyyy.MM.dd}"
              }
            }
          ]
        }
      }
    ]
  },
  "Configuration": {
    "FromEnvironment": "Development environment"
  }
}
